{
  "name": "@vue-macros/short-vmodel",
  "version": "1.2.5",
  "packageManager": "pnpm@7.32.0",
  "description": "short-vmodel feature from Vue Macros.",
  "keywords": [
    "vue-macros",
    "macros",
    "vue",
    "sfc",
    "setup",
    "script-setup",
    "short-vmodel"
  ],
  "license": "MIT",
  "homepage": "https://github.com/sxzz/unplugin-vue-macros#readme",
  "bugs": {
    "url": "https://github.com/sxzz/unplugin-vue-macros/issues"
  },
  "repository": {
    "type": "git",
    "url": "git+https://github.com/sxzz/unplugin-vue-macros.git",
    "directory": "packages/short-vmodel"
  },
  "author": "三咲智子 <sxzz@sxzz.moe>",
  "files": [
    "dist"
  ],
  "main": "dist/index.js",
  "module": "dist/index.mjs",
  "types": "dist/index.d.ts",
  "exports": {
    ".": {
      "dev": "./src/index.ts",
      "types": "./dist/index.d.ts",
      "require": "./dist/index.js",
      "import": "./dist/index.mjs"
    },
    "./vite": {
      "dev": "./src/vite.ts",
      "types": "./dist/vite.d.ts",
      "require": "./dist/vite.js",
      "import": "./dist/vite.mjs"
    },
    "./webpack": {
      "dev": "./src/webpack.ts",
      "types": "./dist/webpack.d.ts",
      "require": "./dist/webpack.js",
      "import": "./dist/webpack.mjs"
    },
    "./rollup": {
      "dev": "./src/rollup.ts",
      "types": "./dist/rollup.d.ts",
      "require": "./dist/rollup.js",
      "import": "./dist/rollup.mjs"
    },
    "./esbuild": {
      "dev": "./src/esbuild.ts",
      "types": "./dist/esbuild.d.ts",
      "require": "./dist/esbuild.js",
      "import": "./dist/esbuild.mjs"
    },
    "./*": "./*"
  },
  "scripts": {
    "build": "tsup && tsx ../../scripts/postbuild.mts",
    "dev": "DEV=true tsup"
  },
  "dependencies": {
    "@vue-macros/common": "workspace:~",
    "@vue/compiler-core": "^3.3.0-alpha.9"
  },
  "devDependencies": {
    "@vue/compiler-sfc": "^3.3.0-alpha.9",
    "tsup": "^6.7.0",
    "tsx": "^3.12.6"
  },
  "engines": {
    "node": ">=14.19.0"
  }
}
